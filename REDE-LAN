CONFIGURAÇÃO
enable
configure terminal 
hostname RT-01
line console 0
password  123456
login 
exec-timeout  8 0
exit
vlan 10
Description Vendas
exit
vlan 20
Description Marketing
exit
!
interface vlan 10
Description Vendas
ip address 10.1.10.1 255.255.255.0
no shutdown
!
exit
interface GigabitEthernet 0/0
no shutdown
exit
!
interface GigabitEthernet 0/0.10
#encapsulation  dot1Q 20
ip address 10.1.10.1 255.255.255.0
#encapsulation  dot1Q 10
no shutdown
!
interface GigabitEthernet 0/0.20
encapsulation  dot1Q 20
ip address 10.1.20.1 255.255.255.0
no shutdown

